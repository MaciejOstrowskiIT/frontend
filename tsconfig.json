{
  "compilerOptions": {
    "target": "esnext",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "paths": {

//      "components/*": [
//        "./src/components/*"
//      ],
//      "pages/*": [
//        "./src/pages/*"
//      ],
//      "routes/*": [
//        "./src/routes/*"
//      ],
//      "styles/*": [
//        "./src/styles/*"
//      ],
//      "types/*": [
//        "./src/types/*"
//      ],
//      "utils/*": [
//        "./src/utils/*"
//      ],
//      "interfaces/*": [
//        "./src/interfaces/*"
//      ]
    }
  },
  "include": [
    "src/*",
    "src/**/*"
  ]
}
